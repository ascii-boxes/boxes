BOX xes

author   "Thomas Jensen"
designer "Joan G. Stark <spunk1111@juno.com>"
revision "1.2"
revdate  "February 13, 2021 (Saturday, 14:00h)"
created  "April 09, 1999 (Friday, 20:05h)"
tags     "artwork, box"

sample
            <\/><\/><\/>
        <\/></\></\></\><\/>
        </\>            </\>
     <\/>                  <\/>
     </\>                  </\>
    <\/>                    <\/>
    </\>                    </\>
     <\/>                  <\/>
     </\>                  </\>
        <\/>            <\/>
        </\><\/><\/><\/></\>
    jgs     </\></\></\>
ends

delim ?"

shapes {
    nw  ("     ", "    <", "    <")
    nnw ("   ", "\/>", "/\>")
    n   ("<\/>", "</\>", "    ")
    nne ("   ", "<\/", "</\")
    ne  ("     ", ">    ", ">    ")
    wnw (" <\/>", " </\>")
    w   ("<\/> ", "</\> ")
    wsw (" <\/>", " </\>")
    sw  ("    <", "    <", "jgs  ")
    ssw ("\/>", "/\>", "   ")
    s   ("    ", "<\/>", "</\>")
    sse ("<\/", "</\", "   ")
    se  (">    ", ">    ", "     ")
    ese ("<\/> ", "</\> ")
    e   (" <\/>", " </\>")
    ene ("<\/> ", "</\> ")
}

elastic (n, e, s, w)

END xes


