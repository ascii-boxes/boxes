BOX stark1

author   "Thomas Jensen"
designer "Joan G. Stark <spunk1111@juno.com>"
revision "1.1"
revdate  "February 13, 2021 (Saturday, 14:00h)"
created  "April 05, 1999 (Monday, 21:55h)"
tags     "artwork, box"

sample
    =-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=
    ^   -,                ,-. .          .    ^
    ^    | ,-. ,-. ;-.    `-, |- ,-. ;-. | '  ^
    ^  , | | | ,-| | |    , ; |  ,-| |   |`.  ^
    ^  `-' `-' `-^ ' ^    `-' `- `-^ '   ` `  ^
    ^          Spunk1111@juno.com             ^
    ^      ,-.             .   . . . .        ^
    ^      `-, ;-. . . ;-. | ' | | | |        ^
    ^      , ; | | | | | | |`. | | | |        ^
    ^      `-' |-' `-^ ' ^ ` ` ` ` ` `        ^
    ^          `                              ^
    =-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-=-= jgs
ends

shapes {
    nw ("=") N ("-=") nne ("-") ne ("=    ")
    w  ("^")                     e ("^    ")
    sw ("=") ssw ("-") s ("=-") se ("= jgs")
}

padding {h 1}
elastic (n,s,e, w)

END stark1


