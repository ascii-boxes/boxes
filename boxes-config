sample
    /**
     *  Javadoc Comments
     *
     */
ends

shapes {
    nw ("/*") nnw ("*") n (" ")
     w (" *")
    sw (" *") ssw ("/") s (" ")
}

padding { left 2 }

delimiter ?"
replace "\*/" with "*\/"                 # quote closing comment tags
reverse "\*\\/" to "*/"

elastic (s, w, n)

END javadoc


