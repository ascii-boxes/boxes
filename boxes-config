BOX mouse

author   "Thomas Jensen"
designer "Joan G. Stark <spunk1111@juno.com>"
revision "1.2"
revdate  "February 13, 2021 (Saturday, 14:00h)"
created  "March 18, 1999 (Thursday, 15:27h)"
tags     "artwork, box, sign"

sample
                .--,       .--,
               ( (  \.---./  ) )
                '.__/o   o\__.'
                   {=  ^  =}
                    >  -  <
     ___________.""`-------`"".____________
    /                                      \
    \ o          joan stark        O       /
    /          spunk1111@juno.com          \
    \          ascii art gallery           /
    / http://www.geocities.com/SoHo/7373/  \
    \______________________________________/
                  ___)( )(___  jgs
                 (((__) (__)))
ends

shapes {
    nnw (" ", " ", " ", " ", " ", "_")
    nne (" ", " ", " ", " ", " ", "_")
    ne  (" ", " ", " ", " ", " ", " ")
    nw  (" ", " ", " ", " ", " ", " ")

    sw  ("\\", " ", " ")
    se  ("/", " ", " ")
    ene ("\\")
    wnw ("/")
    e   ("/","\\")
    w   ("\\","/")
    n (" .--,       .--, ",
       "( (  \\.---./  ) )",
       " '.__/o   o\\__.' ",
       "    {=  ^  =}    ",
       "     >  -  <     ",
       "_.\"\"`-------`\"\"._")

    s ("_____________________",
       "     ___)( )(___  jgs",
       "    (((__) (__)))    ")

    ssw ("_"," "," ")
    sse ("_"," "," ")
}

elastic (nne,nnw, sse,ssw, e, w)

padding {
    top 1
    horiz 1
}

END mouse


# ============================================================================


BOX sunset

author   "Thomas Jensen"
designer "Joan G. Stark <spunk1111@juno.com>"
revision "1.1"
revdate  "February 13, 2021 (Saturday, 14:00h)"
created  "March 30, 1999 (Tuesday, 17:10h)"
tags     "artwork, box"

sample
                               .
                          .    |    .
                           \   |   /
                       '.   \  '  /   .'
                         '. .'```'. .'
     <>.............:::::::`.......`:::::::................<>
     <>:   ., .., . . .  . .  .  .  .  .  . joan stark    :<>
     <>:  :,  :.' : : :`.: :.'  `: `: `: `:  spunk1111    :<>
     <>:  ,.; :   `.' : `: : `.  :  :  :  :   @juno.com   :<>
     <>:..................................................:<> jgs
     <><><><><><><><><><><><><><><><><><><><><><><><><><><><>
ends

shapes {
    e (":<>    ")
    w ("<>:")
    sw ("<>:", "<><")
    se (":<> jgs", "><>    ")
    s ("..", "><")
    nw ("   ", "   ", "   ", "   ", "   ", "<>.")
    ne ("       ", "       ", "       ", "       ", "       ", ".<>    ")
    nnw (" ", " ", " ", " ", " ", ".")
    nne (" ", " ", " ", " ", " ", ".")
    n ("           .           ",
       "      .    |    .      ",
      "       \\   |   /       ",
      "   '.   \\  '  /   .'   ",
       "     '. .'```'. .'     ",
       ":::::::`.......`:::::::")
}

elastic (nne,nnw, s, e, w)

END sunset


