BOX unicornthink

author   "Mike Meyer <boxes@meyer.fm>"
revision "1.1"
revdate  "November 08, 2014 (Saturday, 11:08h)"
created  "September 02, 2013 (Monday, 14:43h)"
tags     "artwork, speech"

sample
     _________________________________
    /                                 \
    |     E L E C T R O L Y T E S     |
    \______________________________ '\
                               ()    \\
                                 O    \\  .
                                   o  |\\/|
                                      / " '\
                                      . .   .
                                     /    ) |
                                    '  _.'  |
                                    '-'/    \
ends

shapes {
    nw  (" ")
    wnw ("/")
    w   ("|")
    sw  ("\\",
         " ",
         " ",
         " ",
         " ",
         " ",
         " ",
         " ",
         " ")

    n   ("_")

    s   ("_",
         " ",
         " ",
         " ",
         " ",
         " ",
         " ",
         " ",
         " ")

    ne  ("       ")
    ene ("\\      ")
    e   ("|      ")
    se  ("       ",
         "\\      ",
         "\\\\  .  ",
         "|\\\\/|  ",
         "/ \" '\\ ",
         ". .   .",
         "    ) |",
         " _.'  |",
         "'/    \\")

    sse ("____ '\\",
         "()    \\",
         "  O    ",
         "    o  ",
         "       ",
         "       ",
         "      /",
         "     ' ",
         "     '-")
}

elastic (n, s, e, w)

padding {
    horiz 2
    top 1
}

END unicornthink


