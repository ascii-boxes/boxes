        `"Y888b,d888P"'       `"Y888b,d888P"'       `"Y888b,d888P"'       `"Y888b,d888P"'       `"Y888b,d888P"'
            ``":"''               ``":"''               ``":"''               ``":"''   Veilleux    ``":"''
ends

shapes {
     nw ("        __,:",
         "     ,ad88P`",
         "   ad88888' ",
         " ,d88888P'  ",
         ",d888P\"'    ",
         ":([\"        ")
      w ("`Y88ba,     ",
         " `88888ba   ",
         "  `Y88888b, ",
         "    `\"Y888b,",
         "        \"]):",
         "     ,ad88P'",
         "   ad88888' ",
         " ,d88888P'  ",
         ",d888P\"'    ")
     sw (":([\"        ",
         "`Y88ba,     ",
         " `88888ba   ",
         "  `Y88888b, ",
         "    `\"Y888b,",
         "        ``\":")

      n (",__               __,:",
         "Y88ba,         ,ad88P`",
         "`88888ba     ad88888' ",
         " `Y88888b, ,d88888P'  ",
         "   `\"Y888b,d888P\"'    ",
         "       ``\":\"''        ")

    nne (",__    Normand    __,",
         "Y88ba,         ,ad88P",
         "`88888ba     ad88888'",
         " `Y88888b, ,d88888P' ",
         "   `\"Y888b,d888P\"'   ",
         "       ``\":\"''       ")

     ne (":,__        ",
         "`Y88ba,     ",
         " `88888ba   ",
         "  `Y88888b, ",
         "    `\"Y888b,",
         "        \"]):")
      e ("     ,ad88P'",
         "   ad88888' ",
         " ,d88888P'  ",
         ",d888P\"'    ",
         ":([\"        ",
         "`Y88ba,     ",
         " `88888ba   ",
         "  `Y88888b, ",
         "    `\"Y888b,")
     se ("        \"]):",
         "     ,ad88P'",
         "   ad88888' ",
         " ,d88888P'  ",
         ",d888P\"'    ",
         ":\"''        ")

      s ("       __,:,__        ",
         "    ,ad88P`Y88ba,     ",
         "  ad88888' `88888ba   ",
         ",d88888P'   `Y88888b, ",
         "d888P\"'       `\"Y888b,",
         "\"''               ``\":")

    sse ("       __,:,__       ",
         "    ,ad88P`Y88ba,    ",
         "  ad88888' `88888ba  ",
         ",d88888P'   `Y88888b,",
         "d888P\"'       `\"Y888b",
         "\"''   Veilleux    ``\"")
}

padding {
    vertical 1
    horizontal 3
}

elastic (n, e, s, w)

END normand


