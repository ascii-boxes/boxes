BOX html

author   "Thomas Jensen <boxes(at)thomasjensen(dot)com>"
designer "(public domain)"
revision "1.2"
revdate  "October 19, 2012 (Friday, 17:32h)"
created  "March 18, 1999 (Thursday, 15:26h)"

sample
    <!-- - - - - - - - - - - -->
    <!--                     -->
    <!--                     -->
    <!--                     -->
    <!-- - - - - - - - - - - -->
ends

shapes {
    nw ("<!--") nnw (" ") n ("- ") ne ("-->")
     w ("<!--")                     e ("-->")
    sw ("<!--") ssw (" ") s ("- ") se ("-->")
}

padding { h 1 }

delim ?"
replace "-->" with "-\-\>"
reverse "-\\-\\>" to "-->"

elastic (n,e,s,w)

END html


