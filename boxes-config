    #############
    #           #
    #           #
    #############
ends

shapes {
    nw ("#") n ("#")
    se ("#") e ("#")
    sw ("#") s ("#")
    ne ("#") w ("#")
}

padding { h 1 }

elastic (n,s,e, w)

END shell


