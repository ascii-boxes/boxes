BOX fence

designer "Dmitry Bolshakov <bdimych@protonmail.com>"
author   "Dmitry Bolshakov <bdimych@protonmail.com>"
created  "2017-04-10"
revision "1.0"
tags     "artwork, box"

sample
     /\ /\ /\ /\ /\
    |__|__|__|__|__|
    |  |  |  |  |  |
    |  | fence  |  |
    |__|__|__|__|__|
    |__|__|__|__|__|
ends

delim @"

shapes {
    nw (   " /\ /"   ,
           "|__|_"   ,
           "|  | "   )
    w  (   "|  | "   )
    sw (   "|__|_"   ,
           "|__|_"   )

    ne (   "\ /\ "   ,
           "_|__|"   ,
           " |  |"   )
    e  (   " |  |"   )
    se (   "_|__|"   ,
           "_|__|"   )

    n (  "\ /"  ,
         "_|_"  ,
         " | "  )

    s (  "_|_"  ,
         "_|_"  )
}

elastic (n, e, s, w)

END fence


